{
  "name": "wiki-site",
  "version": "1.0.0",
  "description": "Website for Wiki.js",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently 'npm:watch-scss' 'npm:watch-html'",
    "watch-scss": "parcel watch src/scss/style.scss -d dist/assets",
    "watch-html": "cpx 'src/**/*.{html,js,svg,png,jpg,gif,css,ttf,woff,woff2,eot}' dist --watch",
    "build": "parcel build src/scss/style.scss --no-cache -d dist/assets && cpx 'src/**/*.{html,js,svg,png,jpg,gif,css,ttf,woff,woff2,eot}' dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Requarks/wiki-site.git"
  },
  "author": "Nicolas Giard",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Requarks/wiki-site/issues"
  },
  "homepage": "https://github.com/Requarks/wiki-site#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "desandro-matches-selector": "^2.0.2",
    "ev-emitter": "^1.1.1",
    "fizzy-ui-utils": "^2.0.7",
    "get-size": "^2.0.3",
    "isotope": "^1.0.0-alpha.3",
    "jquery": "^3.3.1",
    "masonry-layout": "^4.2.2",
    "parcel-bundler": "^1.11.0",
    "popper.js": "^1.14.7"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-preset-es2015": "^6.24.1",
    "sass": "^1.17.2"
  }
}
